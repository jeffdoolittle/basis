  <ItemGroup>
    <PackageReference Include="microsoft.extensions.dependencyinjection" Version="2.2.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Basis.Core\Basis.Core.csproj" />
  </ItemGroup>

  <ItemGroup>
